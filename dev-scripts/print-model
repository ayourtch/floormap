#!/bin/sh
diesel print-schema --database-url db/floor.sqlite3 | sed -e 's#^}##g' -e 's/^table! {/#[serde(default)]#[derive(Queryable, Insertable, Serialize, Deserialize, Debug, Clone, Default, PartialEq)]/g' -e 's@[ ]\+\(\S\+\) [(][^)]\+[)] {@#[table_name = "\1"]\npub struct\n    \1 {@' -e 's#^        #        pub #g' -e 's# -> Integer#: i32#g' -e 's# -> Nullable<Integer>#: Option<i32>#' -e 's# -> Bool#: bool#g' -e 's# -> Nullable<Bool>#: Option<bool>#' -e 's# -> Text#: String#g' -e 's# -> Nullable<Text>#: Option<String>#' -e 's# -> Timestamp#: NaiveDateTime#g' -e 's# -> Nullable<Timestamp>#: Option<NaiveDateTime>#' -e 's#s {$# {#' -e 's#Switche {#Switch {#' -e 's#Addresse {#Address {#' -e '/allow_tables_to_appear_in_same_query!/,$d' -e '1i use diesel::types::*; use std; use chrono; use chrono::NaiveDateTime; use diesel; use diesel::connection::TransactionManager; use diesel::pg::PgConnection; use diesel::prelude::*; use diesel::sqlite::SqliteConnection; use crate::schema; use schema::*; use serde_derive;' | rustfmt --

